[user]
name=
email=
[color]
diff = auto
status = auto
branch = auto
interactive = true
ui = true
[alias]
co = checkout
ci = commit
st = status
br = branch
hist = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%dCreset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
type = cat-file -t
dump = cat-file -p
[core]
autocrlf = false
